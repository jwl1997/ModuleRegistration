      <!-- Nav Item - Appeal -->
      <li class="nav-item" >
        <a class="nav-link" href="appeal">
          <span style="font-size: 1.0em;">Appeals</span></a>
      </li>

    </div>

    <div class="adm-nav-hide1">
      <!-- Nav Item - register_module -->
      <li class="nav-item" >
